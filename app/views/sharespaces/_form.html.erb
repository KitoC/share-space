
<%= semantic_form_for sharespace_new, html: { class: "address_form" } do |f| %>


  <%= f.inputs name: "New Share space", id: "" do %>

  <%= f.input :space_type %>
  <%= f.input :description,  as: :text, input_html: { class: "text_box_m" } %>
  <%= f.input :cost %>
  <%= f.input :sharespace_venue_id, as: :hidden, input_html: { value: sharespace_venue  } %>


  <% end %>
  <%= f.actions do %>
     <%= f.action :submit, :label => "List your venue.", button_html: { class: "button_fl button" }%>
   <% end %>
<% end %>
