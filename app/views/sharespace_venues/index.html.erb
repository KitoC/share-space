  <div class="nav_box_ss_page">
    <div class="search_bar_ss_page">
        <%= render 'search_address' %>
        <%= link_to 'New Collab Space Listing', new_sharespace_venue_path, class: "new_sharespace" if user_signed_in? %>
    </div>
  </div>

<% if @nearest_me %>
<p> Your closest listing is <%= link_to @nearest_me.name, sharespace_venues_url(@nearest_me.id) %> in <%= @nearest_me.address.city_suburb %></p>
<% end %>
 <p style="background:red;"><%= notice %></p>
<div class="listing_layout">
<div class="listings">

  <h3>Featured Share-Spaces in your area.</h3>
  <div class="tiles_ss_page">
    <% count = 5 %>
    <% until count == 0 %>
    <div class="item_ss_page">
      <div class="tile_ss_page_image"><%= image_tag("default-image.png") %></div>
      <h4>Co-space name</h4>
      <p>Price</p>
      <span class='fa fa-star checked_star'></span>
      <span class='fa fa-star checked_star'></span>
      <span class='fa fa-star checked_star'></span>
      <span class='fa fa-star checked_star'></span>
      <span class='fa fa-star checked_star'></span>

    </div>
    <% count -= 1 %>
    <% end %>
  </div>

  <h3>Share-Spaces.</h3>

  <div class="tiles_ss_page">
  <% @addresses.each do |address| listing = address.addressable %>
      <div class="item_ss_page">
        <div class="tile_ss_page_image"><%=  link_to (profile_photo_display(address.addressable, "medium")), listing %></div>
        <h4><%= link_to listing.name, listing %></h4>
        <p style="margin-top: 0;"><%= link_to address(listing), listing %></p>
        <p><%= price_range(listing) %></p>
        <% rating = rating_average(address.addressable) %>
        <% if rating == "No reviews yet." %>
          <p><%= rating %></p>
        <% else %>
          <% anti_rating = 5 - rating  %>

        <% until rating == 0 %>
          <span class='fa fa-star checked_star'></span>
          <% rating -= 1 %>
        <% end %>

          <% until anti_rating == 0 %>
            <span class='fa fa-star'></span>
            <% anti_rating -= 1 %>
          <% end %>
          <span class="rating_num"><%= listing.reviews.count %></span>
        <% end %>
      </div>


  <% end %>
</div>


</div>
<div id="map"></div>

</div>
   <script>
     var map;
     function initMap() {
       map = new google.maps.Map(document.getElementById('map'), {
         zoom: 15,
         <% if SharespaceVenue.first != nil && SharespaceVenue.first.address.latitude != nil %>
         center: new google.maps.LatLng(<%= SharespaceVenue.first.address.latitude %>, <%= SharespaceVenue.first.address.longitude %>),
         <% else %>
         center: new google.maps.LatLng(-27.4698, 153.0251),
         <% end %>
         mapTypeId: 'roadmap'
       });
// THIS SECTION IS FOR ICONS/Images
       var iconBase = 'https://maps.google.com/mapfiles/kml/shapes/';


// THIS SECTION IS FOR all the iterated locations

       var features = [
         <% @addresses.each do |address| %>

           <% if address.longitude != nil %>
         {
           position: new google.maps.LatLng(<%= address.latitude %>, <%= address.longitude %>),
           // <% if address.addressable.photos.first != nil %>
           // url: '<%= address.addressable.photos.first.image_url %>',
           // scaledSize : new google.maps.Size(35, 35),
           // <% end %>

         },
         <% end %>
         <% end %>
       ];

       // Create markers.
       features.forEach(function(feature) {
         var marker = new google.maps.Marker({
           position: feature.position,
           icon: {url: feature.url,
                  scaledSize : new google.maps.Size(35, 35),
                    },
           map: map
         });
       });
     }
   </script>
   <script async defer
   src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC_u77wbZStldL82_gGDInvPlanmANOyl8&callback=initMap">
   </script>
