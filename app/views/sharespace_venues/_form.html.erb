

<%= semantic_form_for sharespace_venue, html: { class: "address_form" } do |f| %>


  <%= f.inputs name: "Sharespace listing.", id: "" do %>

  <%= f.input :name %>
  <%= f.input :description,  as: :text, input_html: { class: "text_box_m" } %>
  <%= f.input :rules, as: :text, input_html: { class: "text_box_m" } %>
  <%= f.input :surrounding_area_description, as: :text, input_html: { class: "text_box_m" } %>
  <%= f.input :user_id, as: :hidden, input_html: { value: current_user.id  } %>

  <% end %>
  <%= f.actions do %>
     <%= f.action :submit, :label => "Submit.", button_html: { class: "button_fl button" }%>
   <% end %>
<% end %>
